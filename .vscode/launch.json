// .vscode/launch.json

{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "debug attach via npm run debug",
      "request": "launch",
      "runtimeArgs": ["run-script", "debug"],
      "runtimeExecutable": "npm",
      "skipFiles": ["<node_internals>/**"],
      "type": "pwa-node"
    }
  ]
}
